{
    "name": "H.264",
    "ffmpeg_arguments": [
        "-pix_fmt",
        "yuv420p",
        "-c:v",
        "h264_nvenc",
        "-rc:v",
        "cbr_ld_hq",
        "-b:v",
        "#max_file_size#",
        "-vsync",
        "2",
        "-c:a",
        "aac",
        "-b:a",
        "128k"
    ],
    "extension": ".mov",
    "max_file_size": "20"
}